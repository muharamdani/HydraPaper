<interface>
  <object class="GtkAboutDialog" id="aboutdialog">
    <property name="can_focus">False</property>
    <property name="program_name">@prettyname@</property>
    <property name="version">@VERSION@</property>
    <property name="copyright" translatable="yes">@authorfullname@, et al.</property>
    <property name="website">@PACKAGE_URL@</property>
    <property name="authors">@CONTRIBUTORS@</property>
    <property name="translator_credits" translatable="yes">@TRANSLATORS@</property>
    <property name="artists">@DESIGNERS@</property>
    <property name="logo_icon_name">@APPID@</property>
    <property name="license_type">gpl-3-0</property>
  </object>
</interface>
